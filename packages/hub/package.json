{
	"name": "@huggingface/hub",
	"version": "0.0.2",
	"description": "Utilities to interact with the Hugging Face hub",
	"repository": "https://github.com/huggingface/huggingface.js.git",
	"publishConfig": {
		"access": "public"
	},
	"main": "dist/index.js",
	"types": "index.ts",
	"source": "index.ts",
	"scripts": {
		"format": "prettier --write . && eslint --quiet --fix --ext .cjs,.ts .",
		"prepublishOnly": "npm run build",
		"build": "npm run clean && tsc",
		"clean": "rm -Rf dist",
		"test": "npm run build && cross-env NODE_ENV=test mocha --recursive --exit --unhandled-rejections=strict dist/src/lib/**/*.spec.js --timeout 10000"
	},
	"files": [
		"src",
		"dist",
		"index.ts",
		"tsconfig.json"
	],
	"keywords": [],
	"author": "Hugging Face",
	"license": "MIT",
	"devDependencies": {
		"@types/mocha": "^10.0.1",
		"@types/node": "^18.13.0",
		"@typescript-eslint/eslint-plugin": "^5.51.0",
		"@typescript-eslint/parser": "^5.51.0",
		"cross-env": "^7.0.3",
		"eslint": "^8.33.0",
		"eslint-config-prettier": "^8.6.0",
		"eslint-plugin-prettier": "^4.2.1",
		"mocha": "^10.2.0",
		"prettier": "^2.8.3",
		"ts-node": "^10.9.1",
		"type-fest": "^3.5.6",
		"typescript": "^4.9.5"
	}
}
